temp=int(input("enter temperature:"))
units=input("enter units:")
f1=temp+(9/5)+32
#print(f1)
k1=273.15+temp
#print(k1)
c1=(temp-32)*(5/9)
#print(c1)
k2=((temp-32)*(5/9))+273.15
#print(k2)
c2=temp-273.15
#print(c2)
f2=(temp-273.15)*(9/5)+32
#print(f2)
if units== "c":
    print(f1)
    print(k1)
elif units== "f":
    print(c1)
    print(k2)
else:
    print(c2)
